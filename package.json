{
  "name": "vuttr",
  "version": "0.0.1",
  "description": "Very Useful Tools to Remember",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "eslint": "eslint --ext=js --ext=jsx --fix ./src",
    "stylelint": "stylelint \"src/**/*.css\"",
    "prettier": "prettier --config ./.prettierrc.json --write \"src/**/*{.js,.jsx,.css}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielbarion/VUTTR.git"
  },
  "keywords": [
    "VUTTR",
    "BossaBox",
    "JS",
    "JavaScript",
    "Challenge"
  ],
  "author": "Daniel Barion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielbarion/VUTTR/issues"
  },
  "homepage": "https://github.com/danielbarion/VUTTR#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "husky": "^4.0.10",
    "lint-staged": "^10.0.2",
    "postcss-custom-properties": "^9.0.2",
    "postcss-import": "^12.0.1",
    "prettier": "^1.19.1",
    "react-app-rewire-postcss": "^3.0.2",
    "react-app-rewired": "^2.1.5",
    "react-scripts": "^3.3.0",
    "react-test-renderer": "^16.12.0",
    "stylelint": "^13.0.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^19.0.0",
    "url-loader": "^3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "classnames": "^2.2.6",
    "dotenv-flow": "^3.1.0",
    "next": "^9.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx}": [
      "eslint"
    ],
    "*.css": [
      "stylelint"
    ]
  }
}
